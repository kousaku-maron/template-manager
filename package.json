{
  "name": "@kousaku-maron/template-manager",
  "version": "0.1.1",
  "description": "Scaffold projects from repository templates.",
  "type": "module",
  "bin": {
    "template-manager": "./bin/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
